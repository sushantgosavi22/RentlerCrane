<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">


    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.sushant.sampledemomvvmapicall.views.rentlerdashboard.ui.vehicle.VehicleFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_vehicle" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.sushant.sampledemomvvmapicall.views.rentlerdashboard.ui.assignment.AssignmentFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_assignment" >

        <argument
            app:nullable="true"
            android:defaultValue="@null"
            android:name="vehicle"
            app:argType="com.sushant.sampledemomvvmapicall.views.vehicaldetail.Vehicle"/>

    </fragment>

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.sushant.sampledemomvvmapicall.views.rentlerdashboard.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>